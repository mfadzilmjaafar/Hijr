
@{
    ViewBag.Title = "Pertanyaan Surat Tawaran";
}
@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <h4>Pertanyaan.</h4>
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        <label for="icinput" class="col-md-2 control-label">No KP Semasa</label>
        <div class="col-md-10">
            <input class="form-control" id="icinput" type="text" value="">
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Hantar" />
        </div>
    </div>
}